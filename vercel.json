{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "/(.*)",
      "use": "@vercel/node",
      "config": { "includeFiles": "dist/**" }
    }
  ]
}
